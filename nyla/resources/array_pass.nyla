
int giveToMe(int[][][] a) {
	int s = 0;
	for int i = 0; i < a.length[0]; i += 1 {
		for int j = 0; j < a.length[1]; j += 1 {
			for int k = 0; k < a.length[2]; k += 1 {
				s += a[i][j][k];
			}
		}
	}
	int[] g = a[1][0];
	return s + g[1];
}

int main() {

	int[][][] a = { { {3, 6}, { 4 } }, { {7, 8, 4} } };
	
	int[][] b = a[0];
	int s = giveToMe(a);
	
	return s + b[1][0];
}