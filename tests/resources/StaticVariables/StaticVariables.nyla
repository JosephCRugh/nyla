module InnerModule {
	int c = 44;
	int[] g = {99,98,97,96};
}

module StaticVariables {

	static int func(int y) {
		return y * 4;
	}
	
	static int a = 4;
	static int b = func(77);
	static int c;
	static int[4] d = {55,124};
	
	int x1 = 14;
	int x2 = 241 + 52; // Testing to make sure folding occures
	int x3;
