module ArrayPass {

	public static int giveToMe(int[][][] a) {
		int s = 0;
		for int i = 0; i < a.length; i += 1 {
			for int j = 0; j < a[i].length; j += 1 {
				for int k = 0; k < a[i][j].length; k += 1 {
					s += a[i][j][k];
				}
			}
		}
		int[] g = a[1][0];
		return s + g[1];
	}

	public static int main() {
		int[][][] a = { { {3, 6}, { 4 } }, { {7, 8, 4} } };
		
		int[][] b = a[0];
		int s = giveToMe(a);
		
		return s + b[1][0];
	}
}